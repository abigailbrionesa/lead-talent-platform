generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model form_responses {
  id           Int       @id @default(autoincrement())
  name         String?
  email        String?
  age          Int?
  submitted_at DateTime? @default(now()) @db.Timestamp(6)
}
